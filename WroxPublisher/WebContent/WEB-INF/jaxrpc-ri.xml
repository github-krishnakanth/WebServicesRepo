<webServices
    xmlns="http://java.sun.com/xml/ns/jax-rpc/ri/dd"
    version="1.0"
    targetNamespaceBase="http://wrox.com/publications/service"
    typeNamespaceBase="http://wrox.com/publications/types"
    urlPatternBase="/bookInfo">

    <endpoint
        name="BookInfo"
        displayName="Book Information Service"
        description="Provides the information about books" 
        wsdl="/WEB-INF/BookInfoService.wsdl"
        interface="com.wp.service.BookInfo"  
        implementation="com.wp.service.BookInfoImpl" 
	     model="/WEB-INF/bookinfo-rpc-enc.xml.gz"/>
 
    <endpointMapping
        endpointName="BookInfo"
        urlPattern="/bookInfo"/>

</webServices>



